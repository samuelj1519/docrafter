{
  "name": "markdown-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "Vite7",
    "TypeScript",
    "naive-ui",
    "UnoCSS"
  ],
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10.5.0"
  },
  "scripts": {
    "build": "vite build --mode prod",
    "build:test": "vite build --mode test",
    "cleanup": "sa cleanup",
    "commit": "sa git-commit",
    "commit:zh": "sa git-commit -l=zh-cn",
    "dev": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "gen-route": "sa gen-route",
    "lint": "eslint . --fix",
    "lint:vue": "eslint . --config eslint.vue.config.js --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "sa release",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
  "dependencies": {
    "highlight.js": "^11.11.1",
    "marked": "^16.1.1",
    "naive-ui": "^2.42.0",
    "pinia": "^3.0.3",
    "typecheck": "^0.1.2",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@unocss/postcss": "^66.4.0",
    "@unocss/preset-attributify": "^66.4.0",
    "@unocss/preset-wind3": "^66.4.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-unocss": "0.0.1-alpha.3",
    "eslint-plugin-vue": "^10.4.0",
    "vue-eslint-parser": "^10.2.0",
    "prettier": "^3.6.2",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.2",
    "unocss": "^66.4.0",
    "vite": "^7.0.6",
    "vue-tsc": "^3.0.5"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm sa git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint && git diff --exit-code"
  }
}
